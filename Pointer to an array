#include <iostream>
using namespace std;

int main()
{   int arr[] = {19,20,21};
    
    uint16_t len=sizeof(arr)/sizeof(int);
    cout<<"length of array "<<len<<endl;
    
    for(int i=0; i<len;i++)
    {cout<<*(arr+i)<<endl;}
    
    cout<<arr<<endl;                //array name as a pointer of first element
    cout<<*arr<<endl;
}
